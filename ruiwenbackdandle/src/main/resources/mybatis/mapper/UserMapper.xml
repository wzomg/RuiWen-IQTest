<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zzw.mapper.UserMapper">

    <select id="findUser" resultType="com.zzw.pojo.User">
        select * from t_user where onid = #{onid}
    </select>

    <!-- 设置添加成功后，返回自增的主键 -->
    <insert id="addUser" parameterType="com.zzw.pojo.User"
            keyProperty="uid" useGeneratedKeys="true">
        insert into t_user(onid) values(#{onid})
    </insert>


</mapper>